  "source": "//SPDX-License-Identifier: BUSL-1.1\npragma solidity ^0.8.0;\n\ninterface ISega  {\n    \n    /*------------------------------------------\n    Parent Vault Functions\n    ------------------------------------------*/\n\n    /**************\n    Admin Functions\n    **************/\n    \n    function pauseTransfers() external;\n    function unpauseTransfers() external;\n    function changeTrader (address _newTrader) external;\n    \n    /**********************************************\n    Return of Account Balances to Parent Vault\n    ******************************************** **/\n    function returnToVault(uint256 _amount) external;\n    function returnToVault(address _token, uint256 _amount) external;\n     \n    /*------------------------------------------\n    Trader Execution Functions\n    ------------------------------------------*/\n    \n    /*********************************************\n    Simple Transfer of ETH && Transfer-Approve of ERC20\n    *********************************************/\n    function sendAssets(address payable receiver, uint256 amount) external;\n    function sendAssets(address receiver, address _token, uint256 amount) external;\n    function tokenApprove(address spender, address _token, uint256 amount) external;\n\n    /*********************************************\n    Smart Contract Interactions by trader\n    *********************************************/\n    function callSC(address smartContract, bytes memory data) external;\n    function callSC(address smartContract, bytes memory data, uint256 _value) external;\n    \n    /*********************************************\n    Events\n    *********************************************/\n\n    event Received(address indexed _from, uint256 amount);\n    event SentOutAssets(address indexed _to, address indexed _token, uint256 _amount);\n    event ApprovedTokensOut(address indexed _to, address indexed _token, uint256 _amount);\n    event SmartContractCall(address indexed _SC, bytes4 indexed _method, uint256 _value);\n\n}",
  "type": "interface"
}